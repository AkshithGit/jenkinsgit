pipeline {
    agent any 
    stages {
        stage("Build"){
           steps {
            echo "hi this is a build stage"
            } 
        }

        stage("Test"){
            steps {
                echo "hi this is a Test stage"
                } 
        }
        stage("deploy"){
            steps {
                echo "hi this is a deploy stage"
                } 
        }
    }
}